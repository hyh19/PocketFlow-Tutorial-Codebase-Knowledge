    }

    _queue.cancelAllJobs();

    for (var element in _snackbarList) {
      element._controller.dispose();
    }
    _snackbarList.clear();
  }

  Future<void> closeCurrentJob() async {
    if (_currentSnackbar == null) return;
    await _currentSnackbar!.close();
  }
